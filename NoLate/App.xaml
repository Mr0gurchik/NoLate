<?xml version = "1.0" encoding = "UTF-8" ?>
<Application xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:NoLate"
             x:Class="NoLate.App">
    <Application.Resources>
        <ResourceDictionary>
            <Color x:Key="Primary">#E67E00</Color>
            <Color x:Key="Secondary">#32CD32</Color>
            <Color x:Key="Background">#121212</Color>
            <Color x:Key="Surface">#2D2D30</Color>
            <Color x:Key="OnSurface">#FFFFFF</Color>
            <Color x:Key="OnPrimary">#FFFFFF</Color>
        </ResourceDictionary>
    </Application.Resources>
</Application>

